CREATE TABLE customer(
   user_id   INT  ,
   name CHAR (20) ,
   PRIMARY KEY (user_id),
);

INSERT INTO customer (user_id, name)
VALUES ('12' , 'Manoram');


INSERT INTO customer (user_id, name)
VALUES ('14' , 'Prateek');


INSERT INTO customer (user_id, name)
VALUES ('15' , 'Manoram Taparia');





CREATE TABLE food_order(
   order_id INT,
   user_id INT,
   food_item CHAR(20),
   order_date TIMESTAMP,
);



INSERT INTO food_order( order_id INT, user_id INT, food_item CHAR(20), order_date TIMESTAMP)
