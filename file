CREATE TABLE Users(
   user_id   INT  ,
   full_name VARCHAR (20) ,
   country_code VARCHAR (25) ,
    dob  TIMESTAMP ,       
   PRIMARY KEY (user_id),
);



#Creating Unique Index

ALTER TABLE User
ADD UNIQUE (user_id);



#Creating Constraint
ALTER TABLE User MODIFY full_name VARCHAR NOT NULL;


#Inserting
INSERT INTO User (user_id, full_name, country_code,dob)
